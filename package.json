{
  "name": "aethene",
  "version": "1.0.0",
  "description": "Aethene - The AI Memory Layer API",
  "homepage": "https://aethene.com",
  "main": "dist/server.js",
  "type": "module",
  "overrides": {
    "minimatch": "^10.2.1",
    "glob": "^11.0.0"
  },
  "scripts": {
    "server": "tsx src/server.ts",
    "server:dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration/",
    "test:load": "tsx tests/load/load-test.ts",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "docker:build": "docker build -t aethene:latest .",
    "docker:run": "docker run -p 3006:3006 --env-file .env aethene:latest"
  },
  "keywords": [
    "ai-memory",
    "memory-api",
    "rag",
    "vector-search",
    "embeddings",
    "semantic-search",
    "llm-memory"
  ],
  "author": "Aethene Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akhilponnada/aethene"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "@google/generative-ai": "^0.24.1",
    "@hono/node-server": "^1.19.6",
    "convex": "^1.32.0",
    "dotenv": "^17.2.3",
    "hono": "^4.11.10",
    "openai": "^6.15.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  }
}
